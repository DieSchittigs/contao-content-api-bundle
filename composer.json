{
  "name": "lexprodsas/contao-content-api-bundle",
  "description": "Access Contao content via an easy to use JSON-API",
  "type": "contao-bundle",
  "version": "dev-master",
  "keywords": [
    "contao",
    "api",
    "bundle",
    "json"
  ],
  "require": {
    "php": "^7.0",
    "contao/core-bundle": "^4.13"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "evert/phpdoc-md": "~0.2.0"
  },
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Tobias Duehr",
      "email": "tobias@dieschittigs.de"
    },
    {
      "name": "LEXPROD SAS",
      "email": "alexandre.n@lexprod.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "DieSchittigs\\ContaoContentApiBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "DieSchittigs\\ContaoContentApiBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": false,
      "ocramius/package-versions": true
    }
  }
}
